{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/auth/callback",
      "destination": "/index.html"
    },
    {
      "source": "/logout",
      "destination": "/index.html"
    },
    {
      "source": "/((?!assets/|favicon.ico|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}
